{
  "name": "tachyons-webpack",
  "version": "1.1.0",
  "description": "A boilerplate for using tachyons with webpack",
  "main": "src/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm run browsersync",
    "build": "webpack --config webpack.config.js --debug --progress && rm ./dist/tachyons.js",
    "clean": "rm -rf ./dist",
    "browsersync": "node bin/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tachyons-css/tachyons-webpack.git"
  },
  "keywords": [
    "tachyons",
    "webpack",
    "boilerplate"
  ],
  "author": "Garren Smith <garren@redcometlabs.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tachyons-css/tachyons-webpack/issues"
  },
  "homepage": "https://github.com/tachyons-css/tachyons-webpack#readme",
  "dependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "browser-sync": "^2.16.0",
    "bs-fullscreen-message": "^1.1.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "strip-ansi": "^3.0.1",
    "style-loader": "^0.13.1",
    "tachyons": "^4.5.3",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.2",
    "webpack-dev-server": "^1.16.1"
  }
}
